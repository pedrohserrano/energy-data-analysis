@prefix : <http://ncats.nih.gov/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDataset a dcat:Dataset ;
    dcterms:title "ChEMBL: a large-scale bioactivity database for drug discovery"@en ;
    dcat:distribution :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution .

:accessibility a dqv:Category ;
    skos:definition "Accessibility dimensions involve aspects related to the access, authenticity and retrieval of data to obtain either the entire or some portion of the data for a particular use case."@en ;
    skos:prefLabel "Accessibility dimensions"@en .

:availability a dqv:Dimension ;
    skos:definition "Availability is the extent to which information (or some portion of it) is present, obtainable and ready for use."@en ;
    skos:prefLabel "Availability"@en ;
    dqv:inCategory :accessibility .

:contextualDimensions a dqv:Category ;
    skos:definition "Contextual dimensions are those that highly depend on the context of the task at hand."@en ;
    skos:prefLabel "Contextual dimensions"@en .

:interoperability a dqv:Dimension ;
    skos:definition "Interoperability is the degree to which the format and structure of the information conforms to previously returned information as well as data from other sources."@en ;
    skos:prefLabel "Interoperability"@en ;
    dqv:inCategory :representationalDimensions .

:licensingMetric a dqv:Metric ;
    skos:definition "Machine and human readable indication of the license."@en ;
    skos:prefLabel "Licensing Metric"@en ;
    dqv:expectedDataType xsd:string ;
    dqv:inDimension :availability .

:measurement0001 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :licensingMetric ;
    dqv:value "Applies to: Data use = {Creative Commons Attribution-ShareAlike (CC-BY-SA) 3.0}"^^xsd:string .

:measurement0002 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :scopeAndDatatypesMetric ;
    dqv:value "Approved drug"^^xsd:string .

:measurement0003 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :scopeAndDatatypesMetric ;
    dqv:value "Biomedical Science"^^xsd:string .

:measurement0004 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :scopeAndDatatypesMetric ;
    dqv:value "Peptide"^^xsd:string .

:measurement0005 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :scopeAndDatatypesMetric ;
    dqv:value "Small molecule"^^xsd:string .

:measurement0006 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :terminologyArtifactsMetric ;
    dqv:value "Chemical Entities of Biological Interest"^^xsd:string .

:measurement0007 a dqv:QualityMeasurement ;
    dqv:computedOn :ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution ;
    dqv:isMeasurementOf :terminologyArtifactsMetric ;
    dqv:value "PSI Molecular Interaction Controlled Vocabulary"^^xsd:string .

:relevancy a dqv:Dimension ;
    skos:definition "Relevancy refers to the provision of information which is in accordance with the task at hand and important to the usersâ€™ query."@en ;
    skos:prefLabel "Relevancy"@en ;
    dqv:inCategory :contextualDimensions .

:representationalDimensions a dqv:Category ;
    skos:definition "Representational dimensions capture aspects related to the design of the data."@en ;
    skos:prefLabel "Representational dimensions"@en .

:terminologyArtifactsMetric a dqv:Metric ;
    skos:definition "Fetch the terminology artifacts used by the dataset from FAIRsharing.org."@en ;
    skos:prefLabel "Terminology Artifacts Metric"@en ;
    dqv:expectedDataType xsd:string ;
    dqv:inDimension :interoperability .

:scopeAndDatatypesMetric a dqv:Metric ;
    skos:definition "Fetch the scopes and datatypes present in the dataset from FAIRsharing.org."@en ;
    skos:prefLabel "Scopes and Data Types Metric"@en ;
    dqv:expectedDataType xsd:string ;
    dqv:inDimension :relevancy .

:ChEMBLalargescalebioactivitydatabasefordrugdiscoveryDatasetDistribution a dcat:Distribution ;
    dcterms:title "ChEMBL: a large-scale bioactivity database for drug discovery" ;
    dcat:mediaType "application/rdf" ;
    dqv:hasQualityMeasurement :measurement0001,
        :measurement0002,
        :measurement0003,
        :measurement0004,
        :measurement0005,
        :measurement0006,
        :measurement0007 .

